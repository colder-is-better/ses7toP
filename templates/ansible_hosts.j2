[admin]
node0 ansible_host={{ public_net_prefix }}.{{ first_node_addr }}

[nodes]
{% for k in range(1,node_count) %}
node{{ k }} ansible_host={{ public_net_prefix }}.{{ first_node_addr+k }}
{% endfor %}

[ses_cluster:children]
admin
nodes

[all:vars]
ansible_ssh_user=vagrant
ansible_ssh_private_key_file=~/.vagrant.d/insecure_private_key
ansible_python_interpreter=/usr/bin/python3
